#include<stdio.h>
#include<conio.h>
struct employee
{
    int empid;
    char name[10];
    int salary;
};
void main()
{
    struct employee E;
    printf("enter employee information \n");
    printf("emp id ");
    scanf(" %d"&E.empid );
    printf("employee name");
    scanf("%s",E.name);
    printf("employee salary");
    scanf("%d",&E.salary);
    printf("Empid %d",E.empid);
    printf("Emp name %s",E.name);
    printf("Emp salary %d",E.salary);
}
